            <grant type="Archetype Feature" id="ID_DJ_PSG_ARCHETYPE_FEATURE_DRAGON_EXPANDED_SPELL_LIST_RED" level="1" requirements="ID_DJ_PSG_ARCHETYPE_FEATURE_DRAGON_KIND_RED"/>
